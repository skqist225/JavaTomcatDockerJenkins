    agent any 
    stages {
        stage('Init') {
            steps {
                echo 'Hi, this is me'
                echo 'We are from each other team'
            }
        }
        stage('Build') {
            steps {
                echo 'Builing Sample Maven Project'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying in staing area'
            }
        }
    }
}